<%= form_tag("/materials", method: "post") do %>
  <%= label_tag(:name, "name") %>
  <%= text_field_tag(:name) %>
  <%= label_tag(:quantity, "quantity") %>
  <%= number_field_tag(:quantity) %>
  <%= label_tag(:card_id, "card_id") %>
  <%= number_field_tag(:card_id) %>
  <br>
  <%= submit_tag("create") %>
<% end %>
